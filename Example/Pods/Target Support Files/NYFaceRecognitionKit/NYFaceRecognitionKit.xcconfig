CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/NYFaceRecognitionKit
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 DLIB_JPEG_SUPPORT DLIB_NO_GUI_SUPPORT DLIB_USE_BLAS NDEBUG DLIB_USE_LAPACK
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/../../NYFaceRecognitionKit/Libraries/DLibKit"
OTHER_CFLAGS = $(inherited) -DDLIB_JPEG_SUPPORT -DDLIB_NO_GUI_SUPPORT -DNDEBUG -DDLIB_USE_BLAS -DDLIB_USE_LAPACK
OTHER_LDFLAGS = -l"c++" -l"dlib" -framework "Accelerate" -framework "CoreMedia" -framework "UIKit"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../..
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
VALID_ARCHS = arm64
